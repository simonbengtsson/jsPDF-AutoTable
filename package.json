  },
  "devDependencies": {
    "@webpack-cli/migrate": "0.1.3",
    "jspdf": "1.5.3",
    "mocha": "5.2.0",
    "mock-browser": "0.92.14",
    "mock-require": "3.0.3",
    "object-assign": "4.1.1",
    "object.entries": "1.1.0",
    "ts-loader": "5.3.3",
    "ts-node": "8.0.2",
    "typedoc": "0.14.2",
    "typescript": "3.3.1",
    "uglifyjs-webpack-plugin": "2.1.1",
    "webpack": "4.29.0",
    "webpack-cli": "3.2.1",
    "webpack-dev-server": "3.1.14"
